<h1>Tags</h1>


<table>
  <% @tags.each do |tag| %>
    <tr>
      <td><%= link_to tag.name, tag_path(tag) %></td>
      <td>&nbsp&nbsp&nbsp&nbsp&nbsp</td>
      
      <% if logged_in? %>
        <td><%= link_to "Delete", tag_path(tag), method: :delete, data: {confirm: "Really delete the tag?"} %></td>
      <% end %> 
    </tr>
  <% end %>
</table><br />

<%= link_to "<< Back to Articles List", articles_path %>


